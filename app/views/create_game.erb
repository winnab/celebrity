<h2>Create a game</h2>
<form
  method="POST"
  action="/game_overview"
>
  <fieldset>
    <label
      for="creator_name"
    >
      Your name
    </label>
    <input
      type="text"
      name="creator_name"
      placeholder="Enter your name"
    >
  </fieldset>
  <button type="submit">
    Create Game!
  </button>
</form>
