<form
  method="POST"
  action="/game_overview"
>
  <fieldset>
    <legend>Create a game</legend>
    <label
      for="creator_name"
    >
      Your name
    </label>
    <input
      type="text"
      name="creator_name"
      placeholder="Enter your name"
    >
    <button type="submit">
      Create Game!
    </button>
  </fieldset>
</form>
